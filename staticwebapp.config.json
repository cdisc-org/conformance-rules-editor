{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["PUT", "POST", "PATCH", "DELETE", "GET"],
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/api/*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/aad"
      },
      {
        "route": "/.auth/login/twitter",
        "statusCode": 404
      },
      {
        "route": "/.auth/login/github",
        "statusCode": 404
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "allowedRoles": [
            "authenticated"
        ]
      }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/*.{css,scss,js}"]
      },
    "responseOverrides": {
      "400": {
        "rewrite": "/invalid-invitation-error.html"
      },
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "403": {
        "rewrite": "/custom-forbidden-page.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    },
    "mimeTypes": {
        ".json": "text/json",
        ".js": "text/javascript",
        ".css": "text/css"
    }
  }
